.wrap{
  width: 100%;
  min-height: 100vh;
  padding: 30rpx;
  background: #F6F7FB;
  box-sizing: border-box;
}
.head{
  width: 690rpx;
  display: flex;
  align-items: center;
  margin-bottom: 36rpx;
}
.toSearch{
  font-size: 30rpx;
  color: #9B9B9B;
  width: 110rpx;
  margin-right: 15rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.arrow-down{
  width: 28rpx;
  height: 19rpx;
}
.input-box{
  box-sizing: border-box;
  flex: 1;
  display: flex;
  align-items: center;
  height: 77rpx;
  border-radius: 16rpx;
  background: #fff;
  box-shadow: 0rpx 8rpx 16rpx 0rpx #DBE4F8;
  padding: 17rpx 25rpx;
}
.input-box .phcolor{
  color: #BBBBBB;
  font-weight: 500;
}
.input-box image{
  width: 34rpx;
  height: 34rpx;
  margin-right: 15rpx;
}
.type-list{
  height: 46rpx;
  width: 690rpx;
  display: flex;
  white-space: nowrap;
}
.type-list view{
  display: inline-block;
  margin-right: 44rpx;
  font-size: 26rpx;
  color: #9B9B9B;
  line-height: 37rpx;
}
.type-list view:last-child{
  margin-right: 0;
}
.type-list .active{
  color: #4A90E2;
  position: relative;
}
.type-list .active::after{
  content: '';
  width: 100%;
  height: 4rpx;
  background: #4A90E2;
  position: absolute;
  bottom: -1rpx;
  left: 0;
  border-radius: 2rpx;
}